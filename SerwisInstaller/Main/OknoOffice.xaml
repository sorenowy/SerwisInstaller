        <TextBlock HorizontalAlignment="Left" Height="59" Margin="10,10,0,0" TextWrapping="Wrap" FontStyle="Oblique" FontWeight="DemiBold" Text="Wybierz z listy opcję klienta Lotus lub innego klienta poczty. Zatwierdź OK w celu instalacji lub Anuluj aby pominąć" VerticalAlignment="Top" Width="254"/>
        <ListBox x:Name="listaLotus" HorizontalAlignment="Left" Height="272" Margin="14,90,0,0" VerticalAlignment="Top" Width="250">
            <ListBoxItem Content="Lotus Basic 8.5.1"/>
            <ListBoxItem Content="Lotus Standard 8.5.3"/>
            <ListBoxItem Content="Mozilla Firefox"/>
        </ListBox>
        <Button Content="Button" HorizontalAlignment="Left" Margin="14,375,0,0" VerticalAlignment="Top" Width="120" Height="34"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="139,375,0,0" VerticalAlignment="Top" Width="125" Height="34"/>
    </Grid>
</Window>
